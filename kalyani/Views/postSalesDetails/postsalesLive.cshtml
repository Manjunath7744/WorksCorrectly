@section scripts
        {
    @Scripts.Render("~/bundles/liveRptJqury")

    <Script>

        $(document).ready(function () {
            var dealername  ="@dealer_name";
            var user_role = "@userRole";


            selectAllmultiplePostSalesselectors();
         
            loadPostSalesDashboard();

	     $('.selectpickerAll').multiselect({
		 allSelectedText: 'All',
		enableFiltering : true,
		enableCaseInsensitiveFiltering : true,
		numberDisplayed : 1,
		buttonWidth : '140px',
		maxHeight : 250,
             includeSelectAllOption : true
         });

        });
        function selectAllmultiplePostSalesselectors() {
            $("#LiveWorkshop").multiselect('selectAll', false);
            $("#LiveWorkshop").multiselect('updateButtonText')
            $("#Campaignid").multiselect('selectAll', false);
            $("#Campaignid").multiselect('updateButtonText')
            $("#creLists").multiselect('selectAll', false);
            $("#creLists").multiselect('updateButtonText');
        }
        function loadPostSalesDashboard()
        {
            $.ajax({
                type: "POST",
                url: siteRoot + "/postSalesDetails/PostSalesLiveloadDashboardCounts/",
                data: {},
                cache: false,
                success: function (res) {
                    if (res.success == true) {
                        $('#BoxPostSalesLiveCRE').text(res.BoxPostSalesLiveCRE);
                        $('#BoxPostSalesLiveCalls').text(res.BoxPostSalesLiveCalls);
                        $('#BoxPostSalesLiveCRECalls').text(res.BoxPostSalesLiveCRECalls);
                        $('#BoxPostSalesLiveContact').text(res.BoxPostSalesLiveContact);
                        $('#BoxPostSalesLiveContactPercent').text(res.BoxPostSalesLiveContactPercent);
                        $('#BoxPostSalesLiveComplete').text(res.BoxPostSalesLiveComplete);
                        $('#BoxPostSalesLiveSatisfied').text(res.BoxPostSalesLiveSatisfied);
                        $('#BoxPostSalesLiveDisSatisfied').text(res.BoxPostSalesLiveDisSatisfied);
                    }
                    else {
                        Lobibox.notify('warning', {
                            msg: res.error
                        });
                    }
                },
                error: function (data) {
                    alert("Connection TimeOut! Please Reload the page.");
                }
            });

        }

    </Script>
    }