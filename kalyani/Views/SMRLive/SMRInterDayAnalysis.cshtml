                                    <option value="@v">@v</option>
                                }*@
                            </select>
                        </div>
                    </div>

                </div>
                <div class="modal-footer" style="padding: 2px;margin-top:50px">
                    <!-- <label class="checkbox-inline"><input type="checkbox"  id="SMRmyCheck1" value="">For Data Summary Raw data Click here</label> -->
                    <h5><b style="color:red">Note:- </b> This report is downloadable upto 31 days</h5>
                    <div class="row">
                        <div class="col-xs-4 pull-right">
                            <div class="form-group">
                                <button class="btn btn-success" data-dismiss="modal" id="tblDate" onclick="getDateRangeValuesIntraDayLiveTable();getResultSMRIntradayLive('Location','','SmrIntradayWorkshop');getSMRintradayLive();">Submit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>








