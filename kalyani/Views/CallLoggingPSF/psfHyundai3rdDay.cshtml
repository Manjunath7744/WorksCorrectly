<script>
   
    $(document).ready(function () {
        hideforMaverick();
        var psfType = '@ViewBag.typeOfPSF';

         if (psfType == "7") {
             if ('@Session["DealerCode"].ToString()' == "JDAUTONATION") {
                 $('#interHeading').text('PSF 2ND DISPOSITION FORM');
             }
             else {
                 $('#interHeading').text('PSF 3RD DISPOSITION FORM');
             }
        }
         else if (psfType == "5") {
             if ('@Session["DealerCode"].ToString()' == "JDAUTONATION") {
                 $('#interHeading').text('PSF 9TH DISPOSITION FORM');
             }
             else {
                 $('#interHeading').text('PSF 10TH DISPOSITION FORM');
             }

        }

        $('#imgInter').attr('src', siteRoot + "/public/images/INTERACTION HISTORY.png");
    });

    $('input[name="listingForm.maindisposition"]').click(function () {
        $('#divResolveAction').show();
        $('#PSFYesTalk').hide();
        $('#PSFYesTalkH').hide();
    });


    $('#bkResolved').click(function () {
        $('.ResRemarks').val('');
        $('#resolvedReason').val('');
        $('#divResolveAction').hide();
        $('#PSFconnectCall1H').show();
        $("input:radio[name='listingForm.maindisposition']").each(function (i) {
            this.checked = false;
        });

    });

    $('#btnResolvedValid').click(function () {
        if ($('#resolvedReason').val() == "" || $('#resolvedReason').val() == null) {
            Lobibox.notify('warning', {
                continueDelayOnInactiveTab: true,
                msg: 'Please select any one from reason.'
            });
            return false;
        }
    });
</script>