@section scripts
{
    <Script>
        $(document).ready(function () {


            $('#psfcallfromDate').focus(function () {
                $('#psfcalltoDate').val('');
                $('#billdateFrom').val('');
                $('#billdateTo').val('');

            });
            $('#billdateFrom').focus(function () {
                $('#psfcallfromDate').val('');
                $('#psfcalltoDate').val('');
                $('#billdateTo').val('');

            });


            $(".datepickerfrom").datepicker({
                dateFormat: 'yy-mm-dd',
                changeMonth: true,
                changeYear: true,
                maxDate: 0,

                onSelect: function () {
                    var datepickerto = $('.datepickerto');
                    var startDate = $(this).datepicker('getDate');
                    var minDate = $(this).datepicker('getDate');
                    var dt2Date = datepickerto.datepicker('getDate');
                    //difference in days. 86400 seconds in day, 1000 ms in second
                    var dateDiff = (dt2Date - minDate) / (86400 * 1000);

                    startDate.setDate(startDate.getDate() + 30);
                    if (dt2Date == null || dateDiff < 0) {
                        datepickerto.datepicker('setDate', minDate);
                    }
                    else if (dateDiff > 30) {
                        datepickerto.datepicker('setDate', startDate);
                    }
                    //sets dt2 maxDate to the last day of 30 days window
                    datepickerto.datepicker('option', 'maxDate', startDate);
                    datepickerto.datepicker('option', 'minDate', minDate);
                }
            });
            $('.datepickerto').datepicker({
                dateFormat: 'yy-mm-dd',
                changeMonth: true,
                changeYear: true,
                maxDate: 0,

            });
            $(".datepickerbillfrom").datepicker({
                dateFormat: 'yy-mm-dd',
                changeMonth: true,
                changeYear: true,
                maxDate: 0,

                onSelect: function () {
                    var datepickerto = $('.datepickerbillto');
                    var startDate = $(this).datepicker('getDate');
                    var minDate = $(this).datepicker('getDate');
                    var dt2Date = datepickerto.datepicker('getDate');
                    //difference in days. 86400 seconds in day, 1000 ms in second
                    var dateDiff = (dt2Date - minDate) / (86400 * 1000);

                    startDate.setDate(startDate.getDate() + 30);
                    if (dt2Date == null || dateDiff < 0) {
                        datepickerto.datepicker('setDate', minDate);
                    }
                    else if (dateDiff > 30) {
                        datepickerto.datepicker('setDate', startDate);
                    }
                    //sets dt2 maxDate to the last day of 30 days window
                    datepickerto.datepicker('option', 'maxDate', startDate);
                    datepickerto.datepicker('option', 'minDate', minDate);
                }
            });
            $('.datepickerbillto').datepicker({
                dateFormat: 'yy-mm-dd',
                changeMonth: true,
                changeYear: true,
                maxDate: 0,

            });
            $('.selectpickerAll').multiselect({

                allSelectedText: 'All',
                enableFiltering: true,
                enableCaseInsensitiveFiltering: true,
                numberDisplayed: 1,
                buttonWidth: '140px',
                maxHeight: 250,
                includeSelectAllOption: true
            });

            selectallmultipleDropdown();
        });

        $(".psfmodalclick").click(function () {
            var my_id_value = $(this).data('id');

            document.getElementById('lblcallfromDate').innerHTML = 'CallDate From';
            document.getElementById('lblcalltoDate').innerHTML = 'CallDate To';

            if (my_id_value == "1" || my_id_value == "2" || my_id_value == "3") {
                document.getElementById("psfmodaltitle").innerHTML = "PSF Call History Logs Filters";
                $('#calldateDiv').css({ 'display': 'block' });
                $('#billdateDiv').css({ 'display': 'block' });
                $('#campaignDiv').css({ 'display': 'block' });
                $('#creDivS').css({ 'display': 'block' });
                $('#locationDiv').css({ 'display': 'block' });
                //$('#assigndateDiv').css({ 'display': 'none' });
                $('#ratingwiseDiv').css({ 'display': 'none' });
            }

            else if (my_id_value == "4") {
                document.getElementById("psfmodaltitle").innerHTML = "PSF Data Availablity Filters";

                $('#calldateDiv').css({ 'display': 'none' });
                $('#campaignDiv').css({ 'display': 'none' });
                //$('#assigndateDiv').css({ 'display': 'none' });

                $('#billdateDiv').css({ 'display': 'block' });
                $('#creDivS').css({ 'display': 'block' });
                $('#locationDiv').css({ 'display': 'block' });
                $('#ratingwiseDiv').css({ 'display': 'none' });


            }
            else if (my_id_value == "5") {
                document.getElementById("psfmodaltitle").innerHTML = "PSF VIN Assignment Filters";

                document.getElementById('lblcallfromDate').innerHTML = 'Assign From Date';
                document.getElementById('lblcalltoDate').innerHTML = 'Assign To Date';
                //$('#assigndateDiv').css({ 'display': 'block' });
                $('#campaignDiv').css({ 'display': 'block' });
                $('#billdateDiv').css({ 'display': 'block' });
                $('#creDivS').css({ 'display': 'block' });
                $('#locationDiv').css({ 'display': 'block' });

                $('#calldateDiv').css({ 'display': 'block' });
                $('#ratingwiseDiv').css({ 'display': 'none' });

            }
            else if (my_id_value == "6" || my_id_value == "7" || my_id_value == "8" || my_id_value == "9") {

                if (my_id_value == "6" || my_id_value == "7") {
                    document.getElementById("psfmodaltitle").innerHTML = "PSF Complaint Resolved Filters";
                }
                else if (my_id_value == "8" || my_id_value == "9") {
                    document.getElementById("psfmodaltitle").innerHTML = "PSF  UnResolved Complaint Filters";
                }

                //$('#assigndateDiv').css({ 'display': 'none' });
                $('#campaignDiv').css({ 'display': 'none' });
                $('#billdateDiv').css({ 'display': 'block' });
                $('#creDivS').css({ 'display': 'block' });
                $('#locationDiv').css({ 'display': 'block' });

                $('#calldateDiv').css({ 'display': 'none' });
                $('#ratingwiseDiv').css({ 'display': 'none' });

            }
            else if (my_id_value == "10") {

                $('#calldateDiv').css({ 'display': 'none' });
                $('#campaignDiv').css({ 'display': 'none' });
                //$('#assigndateDiv').css({ 'display': 'none' });

                $('#billdateDiv').css({ 'display': 'block' });
                $('#creDivS').css({ 'display': 'block' });
                $('#locationDiv').css({ 'display': 'block' });
                $('#ratingwiseDiv').css({ 'display': 'block' });


            }



            $(".modal-body #psgbucketID").val(my_id_value);
        })

        function selectallmultipleDropdown() {
            $("#psfworkshop").multiselect('selectAll', false);
            $("#psfworkshop").multiselect('updateButtonText')
            $("#psfcre").multiselect('selectAll', false);
            $("#psfcre").multiselect('updateButtonText')
            $("#psfCampaign").multiselect('selectAll', false);
            $("#psfCampaign").multiselect('updateButtonText')

        }

        function clearAllPSFFilters() {
            selectallmultipleDropdown();
            $('#psfratingWise').prop('selectedIndex', 0);

            $('#psfcallfromDate').val('');
            $('#psfcalltoDate').val('');
            $('#billdateFrom').val('');
            $('#billdateTo').val('');
        }

        function downloadPSFReports()
        {
            var reportId, reportName;
            reportId = $('#psgbucketID').val();

            if (reportId == "10") {
                var reportId = $('#psfratingWise').val();
                var name = $("#psfratingWise option:selected").text();
                reportName = "servicerationreport-" + name;

            }
            else {
                var name = $("#psfmodaltitle").text();
                reportName = name.replace(/[\. ,:-]+/g, "")

            }
            var callfromDate = $('#psfcallfromDate').val();
            var calltoDate = $('#psfcalltoDate').val();
            var billdateFrom = $('#billdateFrom').val();
            var billdateTo = $('#billdateTo').val();

            var workshope_list = $('#psfworkshop option:selected').toArray().map(item => item.value).join();
            var campaign_list = $('#psfCampaign option:selected').toArray().map(item => item.value).join();
            var cre_list = $('#psfcre option:selected').toArray().map(item => item.value).join();
            var connectionId = $.connection.hub.id;


            if ((callfromDate === "" || calltoDate == "") && (billdateFrom === "" || billdateTo == "")) {
                Lobibox.notify('warning', {
                    continueDelayOnInactiveTab: true,
                    msg: "Please Enter  From and To Date."
                });
                return false;
            }
            var downloadFilterLists = { reportId: reportId, workshopLists: workshope_list, campaignLists: campaign_list, creLists: cre_list, billdateFrom: billdateFrom, billdateTo: billdateTo, callfromDate: callfromDate, calltoDate: calltoDate, reportName: reportName, connectionId: connectionId }


            $.when(psfReportDownnloadAjax(downloadFilterLists)).then
                (function successHandler(res) {
                    console.log(res);
                    if (res.success == true) {
                        window.location = siteRoot + "/PSFReports/DownloadALL/?reportName=" + reportName;
                        clearAllPSFFilters();
                    }
                    else if (res.success == false) {
                        Lobibox.notify('warning', {
                            continueDelayOnInactiveTab: true,
                            msg: res.error
                        });
                        return false;
                    }
                    else {
                        console.log(res.error);
                    }

                },
                    function errorHandler(res) {
                        console.log(err);
                    })

        }

        function psfReportDownnloadAjax(downloadFilterLists) {
            return $.ajax({
                url: siteRoot + '/PSFReports/PSFDownloadFilter/',
                type: "post",
                data: { PSFdownloadFilters: JSON.stringify(downloadFilterLists) }
            });
        }
    </Script>
}





